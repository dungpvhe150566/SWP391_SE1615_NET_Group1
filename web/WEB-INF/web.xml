        <servlet-name>CompareProductFinal</servlet-name>
        <url-pattern>/CompareProductFinal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CompareByAjax</servlet-name>
        <url-pattern>/CompareByAjax</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
