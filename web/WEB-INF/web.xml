        <servlet-class>controller.searchInAccountController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CompareProducts</servlet-name>
        <servlet-class>controller.CompareProducts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CompareProductFinal</servlet-name>
        <servlet-class>controller.CompareProductFinal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CompareByAjax</servlet-name>
        <servlet-class>controller.CompareByAjax</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProductsController</servlet-name>
        <url-pattern>/ProductsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShopController</servlet-name>
        <url-pattern>/ShopController</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>ShopController</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>ShopDetailController</servlet-name>
        <url-pattern>/ShopDetailController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/HomeController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewAllFeedbackController</servlet-name>
        <url-pattern>/ViewAllFeedbackController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitFeedbackControl</servlet-name>
        <url-pattern>/SubmitFeedbackControl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewFeedbackDetailController</servlet-name>
        <url-pattern>/ViewFeedbackDetailController</url-pattern>
    </servlet-mapping>
